{
   "Version": "2012-10-17",
   "Statement": [
      {
         "Sid": "Statement1",
         "Effect": "Allow",
         "Principal": {
            "AWS": "arn:aws:iam::{{accountId}}:user/{{username}}"
         },
         "Action": [
            "s3:PutObject",
            "s3:GetObject",
            "s3:DeleteObject"
         ],
         "Resource": [
            "arn:aws:s3:::{{bucketName}}/*",
            "arn:aws:s3:::{{bucketName}}"
         ]
      },
      {
         "Sid": "Statement2",
         "Effect": "Allow",
         "Principal": {
            "AWS": "arn:aws:iam::{{accountId}}:user/{{username}}"
         },
         "Action": "s3:ListBucket",
         "Resource": [
            "arn:aws:s3:::{{bucketName}}/*",
            "arn:aws:s3:::{{bucketName}}"
         ]
      }
   ]
}